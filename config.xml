<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="org.linepack" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>LineMob</name>
  <icon src="www\img\logo.png"/>
  <description>
        Controle das Finan√ßas da empresa LinePack
    </description>
  <author email="leandro_franciscato@hotmail.com" href="http://facebook.com/leandro.franciscato">
        Leandro Franciscato Team
    </author>
  <content src="index.html"/>
  <plugin name="cordova-plugin-whitelist" spec="1"/>
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <preference name="DisallowOverscroll" value="true"/>
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>